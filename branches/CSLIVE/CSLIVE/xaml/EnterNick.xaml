<Local:UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:Local="clr-namespace:CSLIVE"
	mc:Ignorable="d"
	x:Class="CSLIVE.EnterNick" Height="173" Width="387">
        
    <Grid x:Name="LayoutRoot">
        <Rectangle RadiusY="18" RadiusX="18" Fill="#59000000"/>
        <TextBox Margin="69,83,0,0" Text="" TextWrapping="Wrap" BorderBrush="{x:Null}" Foreground="#FFFFD300" SelectionBackground="{x:Null}" SelectionForeground="{x:Null}" Background="#2D6C6C6C" VerticalAlignment="Top" Height="24" Width="253" HorizontalAlignment="Left" x:Name="_TextPannel"/>
        <TextBlock Margin="69,69,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="14" Width="125" HorizontalAlignment="Left"><Run Foreground="#FFFFFFFF" Text="Enter your nickname:"/></TextBlock>
        <Button Height="34" HorizontalAlignment="Right" Margin="0,0,65,28" VerticalAlignment="Bottom" Width="103" Content="OK" Style="{StaticResource ButtonStyle2}" BorderThickness="0,0,0,0" x:Name="_ok"/>
    </Grid>
</Local:UserControl>